{
  "name": "server",
  "version": "1.0.0",
  "description": "gusstap",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "schema:drop": "ts-node ./node_modules/typeorm/cli schema:drop",
    "schema:sync": "ts-node ./node_modules/typeorm/cli schema:sync",
    "migration:run": "npm run typeorm migration:run --dataSource ./src/config/typeorm.config.ts",
    "migration:generate": "npm run typeorm  migration:generate --dataSource ./src/config/typeorm.config.ts ./src/database/migrations/$npm_config_name",
    "migration:create": "npm run typeorm migration:create ./src/database/migrations/$npm_config_name",
    "migration:revert": "npm run typeorm --dataSource ./src/config/typeorm.config.ts migration:revert",
    "seed:generate": "npm run typeorm -- migration:create",
    "seed": "npm run typeorm -- migration:run -d ./src/config/typeorm.config.ts",
    "seed:down": "npm run typeorm -- migration:revert -d ./src/config/typeorm.config.ts",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "author": "Artem Korgulin",
  "license": "ISC",
  "dependencies": {
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/typeorm": "^11.0.0",
    "@types/cache-manager": "^4.0.6",
    "@types/cache-manager-redis-store": "^2.0.4",
    "bcrypt": "^6.0.0",
    "cache-manager": "^7.2.4",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cookie-parser": "^1.4.7",
    "crypto": "^1.0.1",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "npm": "^11.6.1",
    "pg": "^8.16.3",
    "pug": "^3.0.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.27"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^11.1.6",
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/express": "^4.17.13",
    "@types/jest": "^30.0.0",
    "@types/node": "24.7.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/parser": "^5.0.0",
    "globals": "^16.4.0",
    "jest": "^30.2.0",
    "nodemon": "^3.1.10",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^29.4.4",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.1.0",
    "tsx": "^4.20.6",
    "typescript": "5.7.3"
  }
}
